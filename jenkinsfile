pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
echo "BBuilding the Code.........."
pth 'C:/Program Files/apache-maven-3.8.5/bin/mvn'
echo ${pth}
bat "${pth} clean"
}
}
stage('Test') 
{
steps{
echo "Testing the Code.........."
bat "mvn test"
}
}
stage('Compile') 
{
steps{
echo "Compiling the Project.........."
bat "mvn compile"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}
